@model IEnumerable<AEVIWeb.Models.TransactModels>

@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Transact list</h2>

@if (ViewData["STRPARAM"] != null)
{
<p>
    @Html.ActionLink("Set filter", "Filter") | @ViewData["STRPARAM"] )
</p>
}
else
{
<p>
    @Html.ActionLink("Set filter", "Filter")
</p>
}
<table>
    <tr>
        <th>LocalTime</th>
        <th>Pan</th>
        <th>Pos</th>
        <th>Country</th>
        <th>Amount</th>
        <th>Currency</th>
        <th>Product</th>
        <th>Quantity</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>@item.LocalTime</td>
        <td>@item.Pan</td>
        <td>@item.Pos</td>
        <td>@item.Country</td>
        <td>@item.Amount</td>
        <td>@item.Currency</td>
        <td>@item.Product</td>
        <td>@item.Quantity</td>
    </tr>
}

</table>

<p>
    @Paging.PaginNavigator(this.Html, (int)(Html.ViewData["PageNum"]), (int)(Html.ViewData["ItemsCount"]),
        (int)(Html.ViewData["PageSize"]))
</p>

